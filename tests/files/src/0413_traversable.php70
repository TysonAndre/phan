<?php
/** @param iterable $input */
function to_array($input): array {
    if (is_array($input)) {
        echo strlen($input);  // wrong
        return $input;
    } else {
        if (rand() % 2 > 0) {
            return $input;  // wrong
        } else {
            return iterator_to_array($input, true);
        }
    }
}
/** @param iterable $input */
function check_iterable($input) : array {
    if (is_object($input)) {
        var_export($input->nnnext());  // does not exist
        return $input;  // wrong
    } else {
        echo strlen($input);  // wrong
        return $input;
    }
}
