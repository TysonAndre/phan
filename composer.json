{
    "name": "etsy/phan",
    "description": "A static analyzer for PHP",
    "keywords": ["php", "static", "analyzer"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Rasmus Lerdorf"
        },
        {
            "name": "Andrew S. Morrison"
        }
    ],
    "config": {
        "disable-tls": true,
        "secure-http": false,
        "sort-packages": true
    },
    "require": {
        "php": "~5.6.0 || ~7.0.0 || ~7.1.0",
        "nikic/PHP-Parser": "~3.1.0",
        "symfony/console": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {"Phan\\": "src/Phan"}
    },
    "autoload-dev": {
        "psr-4": {"Phan\\Tests\\": "tests/Phan"}
    },
    "bin": ["phan", "phan_client", "tocheckstyle"]
}
